{%- macro add_head_details() -%}
<div class="letter-head">{{ letter_head }}</div>

<div class= "yes_letterhead ">

  <div class="row section-break">
    <div class="col-xs-4 column-break">
      <div class="row  data-field custom_height">
        <div class="col-xs-2" >
          {% if doc.medico == "DRA GREY  CAMARENA" or doc.medico == "DRA GREY CAMARENA" %}
          <label>{{_("Institucion")}}</label>
          </div>
          <div class="col-xs-10 value text-center" >
          {{frappe.db.get_value(doc.consulta_tipo, doc.consulta, "institucion")}}
          {% else %}
           <label>{{_("Consulta Tipo")}}</label>
        </div>
        <div class="col-xs-10 value text-center" >
        {{_(doc.consulta_tipo)}}
        {%endif%}
        </div>
      </div>
      <div class="row  data-field custom_height">
        <div class="col-xs-2" >
          <label>{{_("Paciente")}}</label>
        </div>
        <div class="col-xs-10 value text-center" >
          {{doc.paciente}}
        </div>
      </div>
      <div class="row  data-field custom_height">
        <div class="col-xs-2" >
          <label>{{_("Sexo")}}</label>
        </div>
        <div class="col-xs-10 value text-center" >
          {{_(doc.sexo)}}
        </div>
      </div>
    </div>
    <div class="col-xs-4 column-break">
      <div class="row  data-field custom_height">
        <div class="col-xs-2" >
          <label>{{_("Fecha")}}</label>
        </div>
        <div class="col-xs-10 value text-center" >
          {{frappe.format_date(doc.fecha).upper()}}
        </div>
      </div>
      <div class="row  data-field custom_height">
        <div class="col-xs-2" >
          <label>{{_("Edad")}}</label>
        </div>
        <div class="col-xs-10 value text-center" >
          {{doc.edad}}
        </div>
      </div>
      <div class="row  data-field custom_height">
        <div class="col-xs-2" >
          <label>{{_("Medico")}}</label>
        </div>
        <div class="col-xs-10 value text-right" >
          {{_(doc.medico)}}
        </div>
      </div>
    </div>
    <div class="col-xs-4 column-break">
      <div class="row  data-field custom_height">
        <div class="col-xs-2" >
          <label>{{_("Lab.No.")}}</label>
        </div>
        <div class="col-xs-10 value text-center" >
          {{doc.numero_de_laboratorio or "-"}}
        </div>
      </div>
      <div class="row  data-field custom_height">
        <div class="col-xs-2" >
          <label>{{_("Cedula")}}</label>
        </div>
        <div class="col-xs-10 value text-center" >
          {{frappe.db.get_value("Paciente", {"name":doc.paciente}, "cedula_pasaporte") or "-"}}
        </div>
      </div>
      <div class="row  data-field custom_height">
        <div class="col-xs-2" >
          <label>{{_("Telefono")}}</label>
        </div>
        <div class="col-xs-10 value text-center" >
          {{frappe.db.get_value("Paciente", {"name":doc.paciente}, "telefono") or "-"}}
        </div>
      </div>
    </div>
  </div>

{%-endmacro-%}
